{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "content_scripts": [
        {
            "matches": [
                "*://*.reddit.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}